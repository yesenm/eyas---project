                <div class="container">
                    <div class="row-fluid">
                        <div class="span5 contact-form2 centered">
                            <h1>Iniciar sesión</h1>
                            <form id="contact-form2" action="/authenticate" method="post">
                                <div class="control-group">
                                    <div class="controls">
                                        <input type="text" class="span12 form-control" name="username" placeholder="Escribe tu nombre de usuario" required>
                                    </div>
                                </div>
                                <div class="control-group">
                                    <div class="controls">
                                        <input type="password" class="span12" name="password" placeholder="Escribe tu contraseña" required>
                                    </div>
                                </div>
                                <button type="submit" class="message-btn">Ingresar</button>
                            </form>
                    <label>¿No tienes cuenta? <a href="registro_u">¡Registrate!</a></label>

                        </div>
                    </div>
                </div>
